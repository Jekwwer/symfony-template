{
  "name": "jekwwer/symfony-template",
  "description": "A template for Symfony projects",
  "type": "project",
  "license": "MIT",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": "^8.3.12",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/dbal": "^4.2.1",
    "doctrine/doctrine-bundle": "^2.13.0",
    "doctrine/doctrine-migrations-bundle": "^3.3.1",
    "doctrine/orm": "^3.3.0",
    "phpdocumentor/reflection-docblock": "^5.6.0",
    "phpstan/phpdoc-parser": "^2.0.0",
    "symfony/asset": "^7.1.6",
    "symfony/asset-mapper": "^7.1.8",
    "symfony/console": "^7.1.8",
    "symfony/doctrine-messenger": "^7.1.6",
    "symfony/dotenv": "^7.1.6",
    "symfony/expression-language": "^7.1.6",
    "symfony/flex": "^2.4.7",
    "symfony/form": "^7.1.6",
    "symfony/framework-bundle": "^7.1.6",
    "symfony/http-client": "^7.1.8",
    "symfony/intl": "^7.1.8",
    "symfony/mailer": "^7.1.6",
    "symfony/messenger": "^7.1.8",
    "symfony/mime": "^7.1.6",
    "symfony/monolog-bundle": "^3.10.0",
    "symfony/notifier": "^7.1.6",
    "symfony/process": "^7.1.7",
    "symfony/property-access": "^7.1.6",
    "symfony/property-info": "^7.1.8",
    "symfony/routing": "^7.1.6",
    "symfony/runtime": "^7.1.7",
    "symfony/security-bundle": "^7.1.6",
    "symfony/serializer": "^7.1.8",
    "symfony/stimulus-bundle": "^2.21.0",
    "symfony/string": "^7.1.8",
    "symfony/translation": "^7.1.6",
    "symfony/twig-bundle": "^7.1.6",
    "symfony/ux-turbo": "^2.21.0",
    "symfony/validator": "^7.1.8",
    "symfony/web-link": "^7.1.6",
    "symfony/yaml": "^7.1.6",
    "twig/extra-bundle": "^3.13.0",
    "twig/twig": "^3.14.2"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "importmap:install": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "7.1.*"
    }
  },
  "require-dev": {
    "phpmd/phpmd": "^2.15.0",
    "phpstan/phpstan": "^2.0.1",
    "phpunit/phpunit": "^11.4.3",
    "symfony/browser-kit": "^7.1.6",
    "symfony/css-selector": "^7.1.6",
    "symfony/debug-bundle": "^7.1.6",
    "symfony/maker-bundle": "^1.61.0",
    "symfony/phpunit-bridge": "^7.1.6",
    "symfony/web-profiler-bundle": "^7.1.7"
  }
}
